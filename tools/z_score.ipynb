    "import numpy as np\n",
    "\n",
    "\n",
    "\"\"\"\n",
    "Z-Score归一化\n",
    "\"\"\"\n",
    "\n",
    "x = np.array([12,4,5,7,4])\n",
    "\n",
    "# z-score标准化\n",
    "norma = (x - x.mean()) / x.std()\n",
    "\n",
    "# 归一化之后方差为1\n",
    "print(norma.var())"
   ]
  }
